parameters:
    level: 5
    paths:
        - src
        - tests
    reportUnmatchedIgnoredErrors: false
    treatPhpDocTypesAsCertain: false
    ignoreErrors:
        - '#on an unknown class#'
        - '#not found#'
        - '#has invalid type#'
        - '#is never read#'
        - '#always exists and is not nullable#'
        - '#Access to an undefined property SilverStripe\\Core\\Config#'

